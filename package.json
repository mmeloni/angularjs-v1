{
  "name": "swordfish",
  "version": "0.20.5",
  "description": "Wayonara Front-end app",
  "private": true,
  "main": "index.js",
  "scripts": {
    "svg-sprite": "svg-sprite --symbol --symbol-inline --symbol-example --symbol-example-dest ../doc/wn-icons.showcase.html --shape-transform-svgo svgo.json --symbol-dest assets --symbol-sprite icon-sprite.svg assets/icons/*.svg",
    "presass-compile-app": "npm run sass-update-barrel",
    "sass-compile-app": "node-sass --source-map-embed assets/scss/wayonara.scss assets/css/wayonara.css",
    "sass-compile-app:w": "npm run sass-compile-app -- -w",
    "sass-compile-bootstrap": "node-sass --source-map-embed assets/scss/bootstrap-wayonara.scss --output assets/css",
    "sass-compile-bootstrap:w": "npm run sass-compile-bootstrap -- -w",
    "sass-update-barrel": "node build/update-sass-barrel.js assets/scss/wayonara.scss",
    "sass-update-barrel:w": "node build/update-sass-barrel.js assets/scss/wayonara.scss -- -w",
    "sass": "npm run sass-compile-bootstrap && npm run sass-compile-app",
    "sass:w": "concurrently -k 'npm run sass-compile-bootstrap:w' 'npm run sass-compile-app:w' 'npm run sass-update-barrel:w'",
    "clean:dist": "rimraf dist",
    "clean:e2e": "rimraf build/e2e-compiled",
    "dependencies-update": "yarn install --frozen-lockfile --ignore-engines",
    "predist:all": "npm run dependencies-update && npm run clean:dist && npm run sass-update-barrel && npm run lint:f && npm run test && npm run svg-sprite",
    "predist:prod": "npm run predist:all",
    "dist:prod": "node --stack-size=16000 node_modules/webpack/bin/webpack.js --config app/config/webpack.prod.js --progress --profile --bail --sort-assets-by size",
    "predist:stag": "npm run predist:all",
    "dist:stag": "node --stack-size=16000 node_modules/webpack/bin/webpack.js --config app/config/webpack.stag.js --progress --profile --bail --sort-assets-by size",
    "predist:dev": "npm run predist:all",
    "dist:dev": "node --stack-size=16000 node_modules/webpack/bin/webpack.js --config app/config/webpack.prod.js --progress --profile --bail --sort-assets-by size",
    "start:all": "npm run svg-sprite && concurrently -k 'npm run sass-update-barrel:w' 'webpack-dev-server'",
    "prestart": "npm run dependencies-update && npm run sass-update-barrel && npm run lint",
    "start": "npm run start:all",
    "start:fast": "npm run start:all",
    "preversion": "replace 'Version: v\\d+\\.\\d+\\.\\d+' \"Version: v$npm_package_version\" ./README.md",
    "version": "git add README.md && git commit -m \"Bump version in README.md to v$npm_package_version\"",
    "postversion": "echo VERSION=v$npm_package_version > build/build.properties",
    "pretest": "rimraf doc/coverage",
    "test": "npm run test:w -- --single-run",
    "posttest": "npm run test-e2e:all && npm run coverage",
    "test:w": "karma start app/config/karma.conf.js",
    "coverage": "npm run coverage:remap && npm run coverage:report",
    "coverage:remap": "remap-istanbul -i doc/coverage/coverage.json -o doc/coverage/coverage.json -t json -e node_modules,tests,karma-test-shim.ts",
    "coverage:report": "istanbul report --dir doc/coverage --include doc/coverage/coverage.json",
    "lint": "npm run lint:f -- --force",
    "lint:f": "tslint -c tslint.json  --project tsconfig.json --type-check --exclude app/app.route.ts",
    "pretest-e2e:all": "npm run update-webdriver && npm run clean:e2e && npm run compile-e2e-specs",
    "test-e2e:all": "protractor build/e2e-compiled/config/protractor.conf.js",
    "test-e2e:dev": "npm run pretest-e2e:all  && npm run test-e2e:all",
    "test-e2e:stag": "npm run pretest-e2e:all && npm run test-e2e:all",
    "test-e2e:prod": "npm run pretest-e2e:all && npm run test-e2e:all",
    "precompile-e2e-specs": "mkdirp build/e2e-compiled",
    "compile-e2e-specs": "tsc --project tsconfig.e2e.json",
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'bower_components', 'junction') } catch (e) { }\"",
    "update-webdriver": "./node_modules/protractor/bin/webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nextop/wayonara.git"
  },
  "author": "Wayonara <developers@wayonara.com>",
